# Environment Configuration for FinVoice Frontend
# Copy this file to .env and configure with your actual values

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api/v1
REACT_APP_WS_URL=ws://localhost:8000/ws

# Vapi AI Voice Agent (PUBLIC KEY ONLY - Safe for frontend)
REACT_APP_VAPI_PUBLIC_KEY=584ce1e5-dc84-43b3-9ec1-40099b3c315f

# Feature Flags
REACT_APP_ENABLE_VOICE=true
REACT_APP_ENABLE_VAPI_VOICE=true
REACT_APP_ENABLE_BLOCKCHAIN=true
REACT_APP_ENABLE_TRADING_BOT=true

# External Services
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
REACT_APP_SENTRY_DSN=your-sentry-dsn-for-error-tracking

# Development
REACT_APP_DEBUG=false
GENERATE_SOURCEMAP=false
FAST_REFRESH=true
WDS_SOCKET_HOST=localhost
WDS_SOCKET_PORT=3000
SKIP_PREFLIGHT_CHECK=true
DANGEROUSLY_DISABLE_HOST_CHECK=true

# Security Note: 
# - Only PUBLIC keys should be in this file
# - Private keys are stored securely in backend/.env
# - This file can be safely committed to version control
